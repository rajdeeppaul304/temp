        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Close-up view of Burj Khalifa</li><li><i class='fas fa-check'></i>Abra Ride</li><li><i class='fas fa-check'></i>Sound and Light show</li><li><i class='fas fa-check'></i>Fountain show</li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Tickets for Abra Ride</li><li><i class='fas fa-check'></i>Tickets for Burj Khalifa</li><li><i class='fas fa-check'></i>Transfers</li><li><i class='fas fa-check'></i>Food</li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Close-up view of Burj Khalifa</li><li><i class='fas fa-check'></i>Abra Ride</li><li><i class='fas fa-check'></i>Sound and Light show</li><li><i class='fas fa-check'></i>Fountain show</li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Tickets for Abra Ride</li><li><i class='fas fa-check'></i>Tickets for Burj Khalifa</li><li><i class='fas fa-check'></i>Transfers</li><li><i class='fas fa-check'></i>Food</li></ul>",
