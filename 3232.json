    "elements": [
      {
        "id": "d9bc8d4",
        "elType": "widget",
        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>One-time access to the Snow Park</li><li><i class='fas fa-check'></i>Unlimited access to rides; Zorb ball, Bobsled, Tubing Run, Climbing Wall, Snow Plough Playground,\u00a0Bumper Car and Sliding Hill</li><li><i class='fas fa-check'></i>Fleece gloves</li><li><i class='fas fa-check'></i>unlimited chair lift rides</li><li><i class='fas fa-check'></i>Choose any one\u00a0 between 2 hours slope session / snow bullet</li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>One-time access to the Snow Park</li><li><i class='fas fa-check'></i>Unlimited access to rides; Zorb ball, Bobsled, Tubing Run, Climbing Wall, Snow Plough Playground,\u00a0Bumper Car and Sliding Hill</li><li><i class='fas fa-check'></i>Fleece gloves</li><li><i class='fas fa-check'></i>unlimited chair lift rides</li><li><i class='fas fa-check'></i>Choose any one\u00a0 between 2 hours slope session / snow bullet</li></ul>",
              "_id": "c3a62d4"
            }
          ],
          "title_color": "#000000",
          "_element_id": "inclusion-policies",
          "custom_css": "#inclusion-policies li{\n    list-style: none;\n}",
