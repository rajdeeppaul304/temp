          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Hotel pickup</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Thrilling dune bashing in a 4x4 vehicle</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>quad biking, or a camel ride on request at an extra cost</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Hotel drop off</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All personal expenses</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Lunch</span></li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Hotel pickup</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Thrilling dune bashing in a 4x4 vehicle</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>quad biking, or a camel ride on request at an extra cost</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Hotel drop off</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All personal expenses</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Lunch</span></li></ul>",
