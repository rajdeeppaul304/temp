        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Private transfers by 4x4 cars</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>One-hour dune buggy ride</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Private Dinner setup in the middle of the desert</span></li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Private transfers by 4x4 cars</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>One-hour dune buggy ride</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Private Dinner setup in the middle of the desert</span></li></ul>",
