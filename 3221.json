          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Helmet, gloves and googles</li><li><i class='fas fa-check'></i>Camel riding</li><li><i class='fas fa-check'></i>Soft drinks and water</li><li><i class='fas fa-check'></i>Self driving of the quad bike</li><li><i class='fas fa-check'></i>On sharing basis pickup from hotels and residence</li><li><i class='fas fa-check'></i>Sandboarding</li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Meals and Snacks</li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Helmet, gloves and googles</li><li><i class='fas fa-check'></i>Camel riding</li><li><i class='fas fa-check'></i>Soft drinks and water</li><li><i class='fas fa-check'></i>Self driving of the quad bike</li><li><i class='fas fa-check'></i>On sharing basis pickup from hotels and residence</li><li><i class='fas fa-check'></i>Sandboarding</li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Meals and Snacks</li></ul>",
