            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><strong><span style='text-decoration: underline; color: #ff6600;'>Inclusions</span></strong></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Private yacht cruising</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Music system</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Water/ soft drinks</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Washroom facility</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All other personal expenses</span></li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><strong><span style='text-decoration: underline; color: #ff6600;'>Inclusions</span></strong></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Private yacht cruising</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Music system</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Water/ soft drinks</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Washroom facility</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All other personal expenses</span></li></ul>",
