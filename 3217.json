        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>60 minutes nature safari in a Vintage Land Rover</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Pickup and drop off service</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Traditional Breakfast</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All personal expense</span></li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>60 minutes nature safari in a Vintage Land Rover</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Pickup and drop off service</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Traditional Breakfast</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All personal expense</span></li></ul>",
              "_id": "c3a62d4"
            }
