        "id": "d9bc8d4",
        "elType": "widget",
        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusion</strong></span></p><ul><li><i class='fas fa-check'></i>Get Full Day Access to the best Theme Park IN UAE.</li><li><i class='fas fa-check'></i>Experience the thrilling family rides.</li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Personal expenses</li><li><i class='fas fa-check'></i>Transfer (private transfers available at an additional charge)</li><li><i class='fas fa-check'></i>Food</li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusion</strong></span></p><ul><li><i class='fas fa-check'></i>Get Full Day Access to the best Theme Park IN UAE.</li><li><i class='fas fa-check'></i>Experience the thrilling family rides.</li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Personal expenses</li><li><i class='fas fa-check'></i>Transfer (private transfers available at an additional charge)</li><li><i class='fas fa-check'></i>Food</li></ul>",
              "_id": "c3a62d4"
            }
          ],
