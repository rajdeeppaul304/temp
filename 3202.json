        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>International cuisine buffet with veg and non-veg options</li><li><i class='fas fa-check'></i>Live music performance onboard</li><li><i class='fas fa-check'></i>90 minutes cruising in Dubai Marina</li></ul><p><span style='color: #ff6600;'><b><u>Exclusions</u></b></span></p><ul><li><i class='fas fa-check'></i>Personal Expenses</li><li><i class='fas fa-check'></i>Transfers (available upon selection)</li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>International cuisine buffet with veg and non-veg options</li><li><i class='fas fa-check'></i>Live music performance onboard</li><li><i class='fas fa-check'></i>90 minutes cruising in Dubai Marina</li></ul><p><span style='color: #ff6600;'><b><u>Exclusions</u></b></span></p><ul><li><i class='fas fa-check'></i>Personal Expenses</li><li><i class='fas fa-check'></i>Transfers (available upon selection)</li></ul>",
              "_id": "c3a62d4"
            }
          ],
