          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><strong><span style='text-decoration: underline;'><span style='color: #ff6600; text-decoration: underline;'>Inclusions</span></span></strong></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Pick up and drop off</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Complimentary headscarves and stainless steel water bottles that you can take home</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Traditional emirate breakfast</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Nature safari in vintage Land Rover</span></li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><strong><span style='text-decoration: underline;'><span style='color: #ff6600; text-decoration: underline;'>Inclusions</span></span></strong></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Pick up and drop off</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Complimentary headscarves and stainless steel water bottles that you can take home</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Traditional emirate breakfast</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Nature safari in vintage Land Rover</span></li></ul>",
