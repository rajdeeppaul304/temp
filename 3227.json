          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Pick up and drop from the hotel</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>One-hour dune buggy ride</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Picture stop in the desert</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Sheesha at the dessert campsite</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Water, tea, coffee, soft drinks</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Evening snacks</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>BBQ buffet dinner</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Entertainment shows (belly dance, tanoura dance, fire show)</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All personal expense</span></li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Pick up and drop from the hotel</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>One-hour dune buggy ride</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Picture stop in the desert</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Sheesha at the dessert campsite</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Water, tea, coffee, soft drinks</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Evening snacks</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>BBQ buffet dinner</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Entertainment shows (belly dance, tanoura dance, fire show)</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All personal expense</span></li></ul>",
