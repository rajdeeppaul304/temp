    "elements": [
      {
        "id": "d9bc8d4",
        "elType": "widget",
        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions:</strong></span></p><ul><li><i class='fas fa-check'></i>\u00a0<span style='color: #000000;'>30 minutes check-in/changing/safety orientation with a Mammal Specialist</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>30 minutes in the water</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>\u00a0Wetsuits or neoprene vests are provided and are mandatory</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Changing rooms and showers for use during the experience</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Towels and lockers for your use during the experience</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>\u00a0Complimentary juices after the experience</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>FREE same-day access to Aquaventure Waterpark</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Transport</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Food and Beverages</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Personal Expenses</span></li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions:</strong></span></p><ul><li><i class='fas fa-check'></i>\u00a0<span style='color: #000000;'>30 minutes check-in/changing/safety orientation with a Mammal Specialist</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>30 minutes in the water</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>\u00a0Wetsuits or neoprene vests are provided and are mandatory</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Changing rooms and showers for use during the experience</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Towels and lockers for your use during the experience</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>\u00a0Complimentary juices after the experience</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>FREE same-day access to Aquaventure Waterpark</span></li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Transport</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Food and Beverages</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Personal Expenses</span></li></ul>",
              "_id": "c3a62d4"
            }
