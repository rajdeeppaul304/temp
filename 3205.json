        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<div><p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusion</strong></span></p><ul><li><i class='fas fa-check'></i>Aquarium Tunnel</li><li><i class='fas fa-check'></i>Underwater Zoo</li><li><i class='fas fa-check'></i>Penguin Cove &amp; Nursery</li></ul></div><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusion</strong></span></p><ul><li><i class='fas fa-check'></i>Private Transfers (can be arranged at an additional cost)</li><li><i class='fas fa-check'></i>Personal Expenses</li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<div><p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusion</strong></span></p><ul><li><i class='fas fa-check'></i>Aquarium Tunnel</li><li><i class='fas fa-check'></i>Underwater Zoo</li><li><i class='fas fa-check'></i>Penguin Cove &amp; Nursery</li></ul></div><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusion</strong></span></p><ul><li><i class='fas fa-check'></i>Private Transfers (can be arranged at an additional cost)</li><li><i class='fas fa-check'></i>Personal Expenses</li></ul>",
              "_id": "c3a62d4"
