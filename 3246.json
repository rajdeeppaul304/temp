            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline;'><strong><span style='color: #ff6600; text-decoration: underline;'>Inclusions</span></strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Private yacht cruising</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Water/ soft drinks</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>BBQ Grill (BBQ can be done to your food)</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Music system (Bluetooth)</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Washroom facility</span></li></ul><p><span style='text-decoration: underline;'><strong><span style='color: #ff6600; text-decoration: underline;'>Exclusions</span></strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All other personal expenses</span></li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline;'><strong><span style='color: #ff6600; text-decoration: underline;'>Inclusions</span></strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>Private yacht cruising</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Water/ soft drinks</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>BBQ Grill (BBQ can be done to your food)</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Music system (Bluetooth)</span></li><li><span style='color: #000000;'><i class='fas fa-check'></i>Washroom facility</span></li></ul><p><span style='text-decoration: underline;'><strong><span style='color: #ff6600; text-decoration: underline;'>Exclusions</span></strong></span></p><ul><li><i class='fas fa-check'></i><span style='color: #000000;'>All other personal expenses</span></li></ul>",
