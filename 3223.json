    "elements": [
      {
        "id": "d9bc8d4",
        "elType": "widget",
        "settings": {
          "content_width": "full",
          "tabs": [
            {
              "tab_title": "  Inclusions | Exclusions",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Top-level air-conditioning</li><li><i class='fas fa-check'></i>The best quality leather upholstery for the interiors</li><li><i class='fas fa-check'></i>Stereo with surround sound facility</li><li><i class='fas fa-check'></i>Partition wall providing total privacy for the passengers</li><li><i class='fas fa-check'></i>Windows with tinted glasses to ensure privacy</li><li><i class='fas fa-check'></i>Intercom facility to talk to the chauffeur</li><li><i class='fas fa-check'></i>The limousine model is the latest one available in its range</li><li><i class='fas fa-check'></i>Water &amp; Soft drinks Will be provided</li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Snacks and meal</li></ul>",
              "_id": "bbdf2f6"
            },
            {
              "tab_title": "Policies",
              "tab_content": "<p><span style='text-decoration: underline; color: #ff6600;'><strong>Inclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Top-level air-conditioning</li><li><i class='fas fa-check'></i>The best quality leather upholstery for the interiors</li><li><i class='fas fa-check'></i>Stereo with surround sound facility</li><li><i class='fas fa-check'></i>Partition wall providing total privacy for the passengers</li><li><i class='fas fa-check'></i>Windows with tinted glasses to ensure privacy</li><li><i class='fas fa-check'></i>Intercom facility to talk to the chauffeur</li><li><i class='fas fa-check'></i>The limousine model is the latest one available in its range</li><li><i class='fas fa-check'></i>Water &amp; Soft drinks Will be provided</li></ul><p><span style='text-decoration: underline; color: #ff6600;'><strong>Exclusions</strong></span></p><ul><li><i class='fas fa-check'></i>Snacks and meal</li></ul>",
              "_id": "c3a62d4"
            }
          ],
